{
    "ovon": {
    "conversation": {
        "id": "{{ bykResponse.id }}"
    },
    "schema": {
    "version": "{{ schemaVersion }}",
    "url": "{{ schemaUrl }}"
    },
    "sender": {
        "from": "{{ sender }}"
    },
    "responseCode" : 200,
    "events": [
        {
            "eventType": "{{ eventType }}",
            "parameters": {
                "dialogEvent": {
                    "speakerId": "{{ speakerId }}",
                    "span": { "startTime": "{{ getDate }}" },
                "features": {
                    "text": {
                    "mimeType": "text/plain",
                    "tokens": [ { "value": "{{ bykResponse.lastMessage }}" } ]
                    }
                }
            }
        }
    }
    ]
    }
}